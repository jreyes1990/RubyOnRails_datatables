<!DOCTYPE html>
<html>
  <head>
    <title>Databables</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'bootstrap/bootstrap.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'datatables/dataTables.bootstrap4.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'datatables/datatables.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'datatables/fixedHeader.dataTables.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'main', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bootstrap/jquery-3.5.1.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bootstrap/popper.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bootstrap/bootstrap.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/datatables.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/dataTables.buttons.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/dataTables.select.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/dataTables.fixedHeader.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/jszip.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/pdfmake.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/vfs_fonts.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/buttons.html5.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/buttons.print.min.js', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'datatables/buttons.colVis.min.js', 'data-turbolinks-track': 'reload' %>
    
    
  </head>

  <body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
      <!-- Sidebar -->
      <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">
    <%= yield %>
  </ul>
  </div>
  </body>
</html>
